# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.SearchController.index()


#GET		/searchTest/					controllers.Search.searchPublicRepositories(name:String)

GET		/search/:term				controllers.SearchController.SearchRepositoriesUsingInternApi(term:String)

GET		/repositories/:idRepo		controllers.SearchController.getCommitersFromRepo(idRepo:String)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
