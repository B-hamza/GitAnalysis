# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Search.index()


#GET		/searchTest/					controllers.Search.searchPublicRepositories(name:String)

GET		/search/:term				controllers.Search.searchUsingJxRS(term:String)
GET		/search/					controllers.Search.searchUsingJxRS(term:String="")

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
